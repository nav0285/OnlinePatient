<h1>Sign Up</h1>
<%= form_tag('/doctors/2', method: :put) do %>
	<%= label_tag("Id") %>
	<%= text_field_tag(:id) %>
	<%= label_tag("Name") %>
	<%= text_field_tag(:name) %>
	<%= label_tag("Password") %>
	<%= password_field_tag(:password) %>
	<%= label_tag(:password_confirmation, "Password Confirmation") %>
	<%= password_field_tag(:password_confirmation) %>
	<%= submit_tag("Sign Up") %>
<% end %>
